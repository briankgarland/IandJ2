---
// This is only shown on the Our-Range landing page.
// Shows all avaialble categories via CatCards components.

import Layout from "../../layouts/Layout.astro";
import { Breadcrumbs, BreadcrumbsItem } from 'accessible-astro-components';
import Title from "../../components/title.astro";
import CatCards from "../../components/CatCards.astro";

const pageTitle = "Our Range"
const usebgImage = false;
---
<Layout pageTitle={pageTitle}>
  <Breadcrumbs>
    <BreadcrumbsItem 
      href="/"
      label="Home"
    />
    <BreadcrumbsItem
      currentPage={true}
      label={pageTitle}
    />
  </Breadcrumbs>
  <Title pageTitle={pageTitle} usebgImage={usebgImage} />
  <CatCards />
</Layout>

